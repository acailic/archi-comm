    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "echo \"Linting will be implemented\" && exit 0",
    "preview": "tauri build --debug && echo \"Debug build created in src-tauri/target/debug/\"",
