  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf dist node_modules/.vite src-tauri/target/.tauri-cli-cache .vite",
    "clean:all": "rm -rf dist node_modules src-tauri/target .vite && npm install",
    "start": "tauri dev",
    "dev": "vite --config config/vite.config.ts",
    "dev:clean": "npm run clean && vite --config config/vite.config.ts",
    "web:dev": "vite --config config/vite.config.ts",
    "web:build": "vite build --config config/vite.config.ts",
    "build": "tauri build",
    "build:debug": "tauri build --debug",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:dev:clean": "npm run clean && tauri dev",
    "tauri:build": "tauri build",
    "tauri:icon": "tauri icon",
    "release": "semantic-release --config config/.releaserc.json",
    "release:dry": "semantic-release --config config/.releaserc.json --dry-run --no-ci",
    "release:local": "semantic-release --config config/.releaserc.json --no-ci",
    "version:sync": "node tools/scripts/sync-versions.js",
    "postversion": "npm run version:sync",
    "test": "vitest --config config/vite.config.ts",
    "test:run": "vitest run --config config/vite.config.ts",
    "test:watch": "vitest --watch --config config/vite.config.ts",
    "test:coverage": "vitest run --coverage --config config/vite.config.ts",
    "test:coverage:check": "vitest run --coverage --config config/vite.config.ts && node tools/scripts/check-coverage.cjs",
    "test:criterion": "node tools/cli/test-runner.js criterion",
    "test:task": "node tools/cli/test-runner.js task",
    "test:uncovered": "node tools/cli/test-runner.js uncovered",
    "test:generate": "node tools/cli/test-runner.js generate-missing",
    "ac": "node tools/cli/ac-manager.js",
    "ac:list": "node tools/cli/ac-manager.js list",
    "ac:validate": "node tools/cli/ac-manager.js validate",
    "ac:coverage": "node tools/cli/ac-manager.js coverage",
    "review": "node tools/cli/ai-review.js staged",
    "review:file": "node tools/cli/ai-review.js file",
    "review:branch": "node tools/cli/ai-review.js branch",
    "traceability:matrix": "node tools/cli/ac-manager.js matrix",
    "traceability:report": "node tools/cli/ac-manager.js report",
    "lint": "eslint --config config/eslint.config.js . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:precommit": "eslint --config config/eslint.config.js . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 999",
    "lint:fix": "eslint --config config/eslint.config.js . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --config config/.prettierrc --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --config config/.prettierrc --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "e2e": "playwright test --config config/playwright.config.ts",
    "e2e:headed": "playwright test --config config/playwright.config.ts --headed",
    "type-check": "tsc --project config/tsconfig.json --noEmit",
    "validate:structure": "node tools/validate-structure.js",
    "deps:check": "ncu",
    "deps:update": "ncu -i",
    "unused:check": "knip",
    "unused:fix": "knip --fix"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix --max-warnings 999 --report-unused-disable-directives",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
